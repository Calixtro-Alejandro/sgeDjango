    return render(request, 'Inicio/Contacto.html')

def Blog(request):
    return render(request, 'Inicio/Blog.html')

def CyberSeguridad(request):
    return render(request, 'Inicio/CyberSeguridad.html')

def Usuario(request):
    return render(request, 'Inicio/Usuario.html')

